	"browserslist": "> 0.5%, last 2 versions, not dead",
	"source": [
		"src/index.html",
		"src/page2.html"
	],
	"scripts": {
		"dev": "parcel -p 3000",
		"dev:open": "parcel -p 3000 --open",
