		"qs": "^6.11.2"
	},
	"devDependencies": {
		"@parcel/transformer-sass": "2.9.3",
		"@types/alpinejs": "^3.13.3",
		"@types/axios": "^0.14.0",
		"@types/bootstrap": "^5.2.7",
		"@types/eventemitter3": "^2.0.2",
		"@types/jquery": "^3.5.22",
		"@types/lodash": "^4.14.200",
		"@types/qs": "^6.9.8",
